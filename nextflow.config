manifest {
    name = 'XBS-nf'
    description = ''
    version = '0.9.8'
    author = ''
    defaultBranch = 'master'
    homePage = ''
    nextflowVersion = '>=21.10.0'
}

//TODO: Enable after the dev profile is not used
params { includeConfig 'conf/global_params.config' }

profiles {
    dev { includeConfig 'conf/dev.config' }
    docker { includeConfig 'conf/docker.config' }
    pbs { includeConfig 'conf/pbs.config' }
    conda_local { includeConfig 'conf/conda_local.config' }
    optimized_processes { includeConfig 'conf/optimized_processes.config' }
    server { includeConfig 'conf/server.config' }
}
