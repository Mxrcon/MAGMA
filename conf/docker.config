process {

    //----------------------------------------------
    // Custom containers for all deps.
    //----------------------------------------------

    withName:
    'GATK.*|LOFREQ.*|DELLY.*|TBPROFILER.*|MULTIQC.*|FASTQC.*|UTILS.*|FASTQ.*|SAMPLESHEET.*' {
        container = "rg.nl-ams.scw.cloud/xbs-nf-containers/xbs-nf-container-1:cloud"
    }

    withName:
    'QUANTTB.*|BWA.*|IQTREE.*|SNPDISTS.*|SNPSITES.*|BCFTOOLS.*|BGZIP.*|SAMTOOLS.*|SNPEFF.*|CLUSTERPICKER.*' {
        container = "rg.nl-ams.scw.cloud/xbs-nf-containers/xbs-nf-container-2:cloud"
    }

}
